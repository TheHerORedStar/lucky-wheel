    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "envalid": "^7.0.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "fs": "*",
